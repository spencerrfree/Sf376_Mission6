@{
    ViewData["Title"] = "Movie List";
}

@model List<ApplicationResponse>

<table class="table table-bordered table-striped">

    <thead class="font-weight-bold">
        <tr>
            <td>Title</td>
            <td>CategoryId</td>
            <td>Year</td>
            <td>Director</td>
            <td>Rating</td>
            <td>Edited</td>
            <td>Lent To</td>
            <td>Notes</td>
        </tr>

    </thead>
    <tbody>
        @foreach (var x in Model)
            {
        <tr>
            <td>@x.Title</td>
            <td>@x.CategoryId</td>
            <td>@x.Year</td>
            <td>@x.Director</td>
            <td>@x.Rating</td>
            <td>@x.Edited</td>
            <td>@x.LentTo</td>
            <td>@x.Notes</td>
        </tr>
            }
    </tbody>
</table>